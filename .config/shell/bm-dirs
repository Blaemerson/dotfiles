cac ${XDG_CACHE_HOME:-$HOME/.cache}
cf  ${XDG_CONFIG_HOME:-$HOME/.config}
D   ${XDG_DOWNLOAD_DIR:-$HOME/downloads}
doc   ${XDG_DOCUMENTS_DIR:-$HOME/documents}
dt  ${XDG_DATA_HOME:-$HOME/.local/share}
h   $HOME
mus   ${XDG_MUSIC_DIR:-$HOME/music}
mnt  /mnt
pic  ${XDG_PICTURES_DIR:-$HOME/pictures}
bin  $HOME/.local/bin
src $HOME/.local/src
vid  ${XDG_VIDEOS_DIR:-$HOME/videos}
